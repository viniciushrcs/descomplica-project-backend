{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier"
      ],
    "rules": {
        "semi":"off",
        "camelcase": "off",
        "prettier/prettier": ["error", 
            {
              "endOfLine":"auto"
            }
        ],
        "comma-dangle":"off"
    }
}